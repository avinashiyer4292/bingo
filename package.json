{
  "name": "bingo",
  "version": "1.0.0",
  "description": "Bingo game in React, Node.js",
  "main": "index.js",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "cd react-client && npm run start",
    "server": "nodemon server.js --ignore react-client",
    "start-local": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "setup": "npm i && cd react-client && npm i && cd .. ",
    "heroku-postbuild": "cd react-client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-client/node_modules"
  ],
  "keywords": [
    "react",
    "node",
    "bingo"
  ],
  "author": "Avinash Iyer",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.6.1",
    "express": "^4.16.3"
  },
  "dependencies": {
    "body-parser": "^1.18.3"
  }
}
